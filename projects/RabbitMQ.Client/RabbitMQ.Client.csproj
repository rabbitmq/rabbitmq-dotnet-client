    <TemplateFile Template="$(AssemblyInfoTok)" OutputFilename="detok.out" Tokens="@(Tokens)" />
    <Move SourceFiles="Tokenized\detok.out" DestinationFiles="$(AssemblyInfoDetok)" />
  </Target>
  <Target Name="CleanDetokenize">
    <Delete Files="@(Detokenized)" />
